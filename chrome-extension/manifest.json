{
  "name": "Node Reload",
  "version": "1.1",
  "manifest_version": 2,
  "description": "automatic updating of the browser from vim using node.js",
  "icons": {
	  "16":"icon_16x16.png",
	  "48":"icon_48x48.png",
	  "128":"icon_128x128.png"
  },
  "background": {
	  "scripts":["jquery.1.7.js","socket.io.js","background.js"]
  },
  "browser_action": {
    "default_icon": "icon_19x19.png",
	"default_popup": "popup.html"
  },
	"content_scripts": [{
		"matches": [
			"http://*/*", "https://*/*"
		],
		"js": ["jquery.1.7.js", "content.js"]
	}],
  "permissions": [
	  "tabs", "http://*/*", "https://*/*"
  ]
}
